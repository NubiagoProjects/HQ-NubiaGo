# Application Configuration
VITE_APP_NAME=NubiaGo Command Center
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Agent network management and business intelligence platform

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# Airtable Configuration
VITE_AIRTABLE_BASE_URL=https://api.airtable.com/v0

# Feature Flags
VITE_ENABLE_AIRTABLE_INTEGRATION=true
VITE_ENABLE_REAL_TIME_SYNC=true
VITE_ENABLE_ADVANCED_ANALYTICS=true
VITE_ENABLE_EXPORT_DATA=true
VITE_ENABLE_NOTIFICATIONS=true

# Development
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_PERFORMANCE_MONITORING=true

# External Services (Optional)
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=